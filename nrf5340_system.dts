		usbd@36000 { /* USBD */
			compatible = "nordic,nrf-usbd";
			reg = <ARMV8M_S_FLAG 0x50036000 0x1000>, <ARMV8M_NS_FLAG 0x40036000 0x1000>;
			interrupts = <0x36 NRF_DEFAULT_IRQ_PRIORITY>;
			num-bidir-endpoints = <0x1>;
			num-in-endpoints = <0x7>;
