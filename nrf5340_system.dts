			status = "disabled";
			label = "QSPI";
		};

		/* skipped: NFCT */

		/* skipped: MUTEX */

		qdec@33000 { /* QDEC0 */
			compatible = "nordic,nrf-qdec";
			reg = <ARMV8M_S_FLAG 0x50033000 0x1000>, <ARMV8M_NS_FLAG 0x40033000 0x1000>;
			interrupts = <0x33 NRF_DEFAULT_IRQ_PRIORITY>;
			status = "disabled";
